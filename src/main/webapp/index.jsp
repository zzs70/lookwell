        function ContAdd() {

            var openId = $("#hidopenId").val();
            var accessToken = $("#hidaccessToken").val();
            var uQName = $("#uQName").val();
            var uQUrl = $("#uQUrl").val();
            var QQS = $("#QQS").val()


            $.ajax({
                type: "POST",
                url: "ashx/JointLogin.ashx",
                data: "openId=" + openId + "&accessToken=" + accessToken + "&uQName=" + uQName + "&uQUrl=" + uQUrl,
                success: function (result) {

                    if (result == "0") {
                        if (QQS == "1") {
                            QC.Login.signOut()
                        } else {
                            window.open("wap/login-lastStep.aspx");
                        }

                    } else if (result == "1") {
                        location.reload()
                    }

                }
            });
        }


        function getQueryString(name) {
            var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
            var r = window.location.search.substr(1).match(reg);
            if (r != null) return unescape(r[2]);
            return null;
        }

    </script>


