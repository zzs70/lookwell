    <script>
        $(function () {
            position($('.head'))

            function position(obj) {
                var top = parseInt(obj.css('top'))
                var vieW = $(window).width(), vieH = $(window).height(), bodyW = parseInt($('body').css('min-width'))
                obj.css({'min-width': bodyW})
                var userAgentInfo = navigator.userAgent;
                if (userAgentInfo.indexOf('Windows') > 0) {
                    if (vieW < bodyW) {
                        $(window).scroll(function () {
                            var t = $(window).scrollTop()
                            obj.css({'position': 'absolute', 'top': t + top})
                        })
                    }
                    $(window).resize(function () {
                        var newW = $(window).width()
                        $(window).scroll(function () {
                            var t = $(window).scrollTop()
                            if (newW < bodyW) {
                                obj.css({'position': 'absolute', 'top': t + top})
                            }
                            if (newW >= bodyW) {
                                obj.css({'position': 'fixed', 'top': top})
                            }
                        })
                    })
                }
            }
        })
    </script>

