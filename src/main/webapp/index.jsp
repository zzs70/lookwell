            }
            if ($('#yanzheng').val() == "") {
                alert("注册码不可以为空");
                return false;
            }
            //document.getElementById('Registered').disabled = true;

            $.ajax({
                url: "/wap/logindo2.aspx",
                type: "post",
                data: "shou=" + $('#shou').val() + "&mima=" + $('#mima').val() + "&yanzheng=" + $('#yanzheng').val(),
                dataType: "json",
                success: function (data) {
                    if (data == "1") {
                        alert('验证码错误！');
                        document.getElementById('Registered').disabled = false;

                    }

                    if (data == "0") {
                        location.reload()
                        document.getElementById('Registered').disabled = false;
                    }


                },
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                }
            });
            return true;
        }

        //密码找回
        function get_mobile() {
            if ($('#shouss').val() != "") {
                if ($("#shouss").val().length > 10) {
                    $.get('/xiugaipassword.aspx', {mobile: jQuery.trim($('#shouss').val())}, function (msg) {
                        alert(jQuery.trim(unescape(msg)));
                        if (msg == '新的密码将以短信形式下发到您的手机请注意查收') {
                            //  $("#picsuiji").attr("src", "/ashx/mobileyan.ashx?id=" + Math.random());
                            RemainTime();
                        }

                    });

                } else {
                    alert("手机格式不正确");
                }
            }
            else {
                alert("手机不能为空");
            }
        };

        //计时
        var iTime = 89;
        var Account;
        function RemainTime() {
            document.getElementById('zphone').disabled = true;
            var iSecond, sSecond = "", sTime = "";
            if (iTime >= 0) {
                iSecond = parseInt(iTime % 60);
                iMinute = parseInt(iTime / 60)
                if (iSecond >= 0) {
                    if (iMinute > 0) {
                        sSecond = iMinute + "分" + iSecond + "秒";
                    } else {
                        sSecond = iSecond + "秒";
                    }
                }
                sTime = sSecond;
                if (iTime == 0) {
                    clearTimeout(Account);
                    sTime = '获取手机验证码';
                    iTime = 59;
                    document.getElementById('zphone').disabled = false;
                } else {
                    Account = setTimeout("RemainTime()", 1000);
                    iTime = iTime - 1;
                }
            } else {
                sTime = '没有倒计时';
            }

            document.getElementById('zphone').value = sTime;
        }

